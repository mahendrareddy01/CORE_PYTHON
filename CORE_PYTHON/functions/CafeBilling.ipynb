    "def calculate_bill(order,menu):\n",
    "    if len(order)==0:\n",
    "        return '0'\n",
    "    else:\n",
    "        s=0\n",
    "        for i in order:\n",
    "            s+=menu[i]\n",
    "        return s\n",
    "menu = {\"chai\": 20, \"coffee\": 30, \"samosa\": 15}    \n",
    "print(calculate_bill(['samosa','coffee'],menu))       \n",
    "\n"
   ]
