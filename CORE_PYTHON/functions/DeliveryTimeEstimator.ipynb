    "def estimate_delivery_time(distance, traffic):\n",
    "    if traffic=='low':\n",
    "        t=(distance/40)*60\n",
    "        return t\n",
    "    elif traffic=='medium':\n",
    "        t=(distance/25)*60\n",
    "        return t\n",
    "    else:\n",
    "        t=(distance/10)*60\n",
    "        return t\n",
    "print(estimate_delivery_time(60,'high'))    \n"
   ]
  }
