    "def calculate_gst_total(prices,gst):\n",
    "    s=0.0\n",
    "    for i in prices:\n",
    "        s+=i\n",
    "    if s==0.0:\n",
    "        return 0.0\n",
    "    else:\n",
    "        gst1=s+(s*gst)/100\n",
    "        return round(gst1)\n",
    "print(calculate_gst_total([1000,500],18))    "
   ]
  }
